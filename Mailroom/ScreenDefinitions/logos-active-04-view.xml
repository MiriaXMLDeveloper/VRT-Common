<?xml version="1.0" encoding="UTF-8"?>
<view name="logos-active-04-view" template="logo-04-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

<labels default="en">
		<locale name="en">
			<label name="forward" value="Forward"/>
			<label name="release" value="Unlock"/>
			<label name="update" value="Lock"/>
		</locale>
</labels>

<section-overrides>	
		
</section-overrides>

<documentRejection>
		<rejectionReasons>
			<reason name="Send Pre-Approval" commentsRequired="false" note="message.preApproval">
			</reason>
			<reason name="More Info Required" commentsRequired="true" note="message.moreInfo">
			</reason>
			<reason name="Create Order" commentsRequired="false" note="message.createOrder">
			</reason>
		</rejectionReasons>
</documentRejection>
	
<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
</hidden-overrides>

<validations>
     <validation name="validation.logostatusvalidator"/>
	 <validation name="validation.logosCustomerIdvalidator"/>
</validations>

</view>


