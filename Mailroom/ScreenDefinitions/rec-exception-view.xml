<?xml version="1.0" encoding="UTF-8"?>
<view name="upload-misc-view" template="receiver-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">


<labels default="en">
		<locale name="en">
			<label name="forward" value="Re-try"/>
		</locale>
</labels>

<property-overrides>
	<text name="poId" readonly="false"/>
	<text name="shippingDocumentId" readonly="false"/>
</property-overrides>

<documentRejection>
		<rejectionReasons>
			<reason name="Reject" commentsRequired="true" note="message.terminate">
			</reason>
		</rejectionReasons>
</documentRejection>

	
	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>
	
	 <validations>
    </validations>
</view>


