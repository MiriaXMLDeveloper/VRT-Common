<?xml version="1.0" encoding="UTF-8"?>
<view name="upload-view" template="portal-faq1-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

<labels default="en">
		<locale name="en">
			<label name="forward" value="Yes"/>
			<label name="reject" value="No"/>
			<label name="release" value="Later"/>
		</locale>
</labels>

<property-overrides>
		<textArea name="custom1.tbdComment2" readonly="true"/>
</property-overrides>

<action-overrides>
			<rejectButton name="reject" visible="true"/>
</action-overrides>
		
	<documentRejection>
		<rejectionReasons>
			<reason name="More Info" commentsRequired="true" note="message.moreInfo">
			</reason>
		</rejectionReasons>
	</documentRejection>

<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
</hidden-overrides>


<validations>
  
</validations>

</view>


