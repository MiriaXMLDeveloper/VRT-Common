<?xml version="1.0" encoding="UTF-8"?>
<view name="portal-nonpo-paid-view" template="portal-nonpo-paid-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd" readonly="true">

        <labels default="en">
		<locale name="en">
			<label name="forward" value="Complete"/>
			<label name="Reject" value="Early Pay"/>
		</locale>
	</labels>
	
	<property-overrides>
		<date name="dueDate" readonly="true" />
	</property-overrides>
	
	<action-overrides>
		
	</action-overrides>

	
	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	<documentRejection>
		<rejectionReasons>
			<reason name="Early Pay" commentsRequired="true" note="message.earlyPayment">
			</reason>
		</rejectionReasons>
	</documentRejection>
</view>

