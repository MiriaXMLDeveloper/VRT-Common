<?xml version="1.0" encoding="UTF-8"?>
<view name="upload-po-terms-view" template="upload-trade-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

<labels default="en">
		<locale name="en">
			<label name="forward" value="Complete"/>
		</locale>
</labels>

<section-overrides>	
		<section name="New Vendor Details" visible="false"/>
</section-overrides>

<property-overrides>
		<date name="dueDate" readonly="false" validation="required"/>
</property-overrides>


<validations>
    <validation name="validation.duedate.check" />
	<validation name="validation.validPaymentTerm"/>
	<!--validation name="validation.vendortype.check"/-->
	<validation name="validation.validPONumber2"/>
</validations>

</view>


