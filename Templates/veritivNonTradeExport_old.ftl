<#setting number_format=",##0.00">
<#setting locale="en_US">
<#list documentsList as document>
	<#list document.documentLines as docline>
<#if document.seqId?exists>${document.seqId?c}</#if>|<#if document.vendorId?exists>${document.vendorId?right_pad(8)}<#else>${""?right_pad(8)}</#if>|<#if document.number?exists>${document.number?right_pad(25)}<#else>${""?right_pad(25)}</#if>|<#if document.date?exists>${document.date?string["yyyy-mm-dd"]?right_pad(10)}<#else>${""?right_pad(10)}</#if>|<#if document.grossAmount?exists>${document.grossAmount?string.number?right_pad(20)}<#else>${""?right_pad(20)}</#if>|<#if document.taxAmount1?exists>${document.taxAmount1?string.number?right_pad(20)}<#else>${""?right_pad(20)}</#if>|<#if document.tbdDate1?exists>${document.tbdDate1?string["yyyy-mm-dd"]?right_pad(10)}<#else>${""?right_pad(10)}</#if>|<#if document.dueDate?exists>${document.dueDate?string["yyyy-mm-dd"]?right_pad(10)}<#else>${""?right_pad(10)}</#if>|<#if document.discountAmount?exists>${document.discountAmount?string.number?right_pad(20)}<#else>${""?right_pad(20)}</#if>|<#if document.poId?exists>${document.poId?right_pad(8)}<#else>${""?right_pad(8)}</#if>|<#if document.tbdString1?exists>${document.tbdString1?right_pad(3)}<#else>${""?right_pad(3)}</#if>|<#if document.voucherComments?exists>${document.voucherComments?right_pad(30)}<#else>${""?right_pad(30)}</#if>|<#if docline.accountCode?exists>${docline.accountCode?right_pad(30)}<#else>${""?right_pad(30)}</#if>|<#if docline.lineTotal?exists>${docline.lineTotal?string.number?right_pad(18)}<#else>${""?right_pad(18)}</#if>|<#if docline.tbdString4?exists>${docline.tbdString4?string?right_pad(30)}<#else>${""?right_pad(30)}</#if>
	</#list>
</#list>