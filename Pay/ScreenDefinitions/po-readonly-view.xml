<?xml version="1.0" encoding="UTF-8"?>
<view name="po-readonly-view" template="po-readpost-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd" readonly="true">
	
	<labels default="en">
		<locale name="en">
			<label name="forward" value="Complete"/>
		</locale>
	</labels>	
	
	<action-overrides>
		<rejectButton name="reject" readonly="true" visable="false"/>
		<button name="forward" command="forward" readonly="true" visable="false" type="submit" isAnchored="false"/>
		<button name="update" command="update" readonly="true" visible="false" type="submit"/>
		<button name="release" command="release" readonly="true" visible="false" type="submit"/>
		<iconAction name="addLineItem" readonly="true" visible="false"/>
		<iconAction name="removeLineItem" readonly="true" visible="false"/>
		<iconAction name="RemoveAllItems" readonly="true" visible="false"/>
		<iconAction name="RemoveMarkedItems" readonly="true" visible="false"/>
		<iconAction name="RemoveUnMarkedItems" readonly="true" visible="false"/>
		

	</action-overrides>
		
	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	<documentRejection>
	
	</documentRejection>

	<validations>
		
	</validations>
</view>
